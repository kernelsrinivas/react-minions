{
  "name": "mdash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
      "android": "react-native run-android --variant=devDebug --appId com.darden.dash.mobile.dev --main-activity MainActivity",
      "android-devRelease": "react-native run-android --variant=devRelease --appId com.darden.dash.mobile.dev --main-activity MainActivity",
      "android-qa": "react-native run-android --variant=qaDebug --appId com.darden.dash.mobile.qa --main-activity MainActivity",
      "android-qaRelease": "react-native run-android --variant=qaRelease --appId com.darden.dash.mobile.qa --main-activity MainActivity",
      "android-prod": "react-native run-android --variant=prodDebug --appId com.darden.dash.mobile --main-activity MainActivity",
      "android-prodRelease": "react-native run-android --variant=prodRelease --appId com.darden.dash.mobile --main-activity MainActivity",
      "ios": "react-native run-ios",
      "start": "react-native start",
      "compile": "tsc",
      "test": "jest --verbose --coverage",
      "test:smoke": "jest -c jest.smoke.config.js --passWithNoTests",
      "test:update": "jest --verbose --coverage --updateSnapshot --passWithNoTests",
      "test:watch": "jest --verbose --watch",
      "test:ci": "jest --ci --reporters=jest-junit --coverage --passWithNoTests --forceExit || exit 0",
      "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
      "pretty": "prettier --config prettier.config.js --write \"src/**/*.{ts, js, tsx}\" \"index.js\" \"App.tsx\" \"__tests__/**/*.js\" \"*.{ts,js}\"",
      "lint": "eslint --ext .js,.ts,.tsx,.jsx .",
      "lint:html": "eslint --ext .js,.ts,.tsx,.jsx . -f html > lint-report.html",
      "lint:md": "eslint --ext .js,.ts,.tsx,.jsx . --format node_modules/eslint-formatter-markdown/markdown.js -o lint-report.md",
      "sonarqube-scanner": "run-script-os",
      "sonarqube-scanner:win32": "FOR /F \"delims=\" %i IN ('git rev-parse --abbrev-ref HEAD') DO @set BRANCH_NAME=%i && node node_modules\\sonarqube-scanner\\dist\\bin\\sonar-scanner -Dsonar.host.url=https://codequality.darden.com/ -Dsonar.projectKey=mdash -Dsonar.projectName=mDash -Dsonar.sources=src -Dsonar.javascript.lcov.reportPaths=coverage/lcov.info -Dsonar.exclusions=src/check-list/api/checkMockData.ts -Dsonar.branch.name=%BRANCH_NAME% -Dsonar.login=01c64b9f78ff60113f88d524f852558de3f91119",
      "sonarqube-scanner:darwin:linux": "npm run test:ci || true && ./node_modules/sonarqube-scanner/dist/bin/sonar-scanner -Dsonar.host.url=http://localhost:3000/ -Dsonar.projectKey=mdash -Dsonar.projectName=mDash -Dsonar.sources=src -Dsonar.javascript.lcov.reportPaths=coverage/lcov.info -Dsonar.exclusions=src/check-list/api/checkMockData.ts -Dsonar.branch.name=`git rev-parse --abbrev-ref HEAD`",
      "sonarqube-scanner:default": "npm run test:ci || true && ./node_modules/sonarqube-scanner/dist/bin/sonar-scanner -Dsonar.host.url=http://localhost:3000/ -Dsonar.projectKey=mdash -Dsonar.projectName=mDash -Dsonar.sources=src -Dsonar.javascript.lcov.reportPaths=coverage/lcov.info -Dsonar.exclusions=src/check-list/api/checkMockData.ts -Dsonar.branch.name=`git rev-parse --abbrev-ref HEAD`",
      "clear:android": "rm -rf ./android/app/build && cd android && ./gradlew clean && rm -rf ~/.gradle/caches/*",
      "clear:ios": "rm -rf ./ios/build",
      "refreshnm": "rm -rf ./node_modules && rm -rf package-lock.json && npm i",
      "clearcache": "yarn cache clean && rm -rf /tmp/haste-map-react-native-packager-* && rm -rf /tmp/metro-bundler-cache-* && watchman watch-del-all",
      "typeorm-create-migration": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create",
      "postinstall": "rn-nodeify --install stream,crypto,vm,events --hack",
      "restful:advanced": "node node_modules/restful-react/dist/bin/restful-react.js import --config restful-api/restful-react.config.js"
  },
  "dependencies": {
      "@expo/vector-icons": "10.1.0",
      "@react-native-community/async-storage": "1.7.0",
      "@react-native-community/masked-view": "0.1.6",
