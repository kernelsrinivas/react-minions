      "react-native-nfc-manager": "2.0.3",
      "react-native-permissions": "2.0.5",
      "react-native-randombytes": "3.5.3",
      "react-native-reanimated": "1.4.0",
      "react-native-safe-area-context": "0.6.4",
      "react-native-screens": "2.0.0-alpha.33",
      "react-native-secure-key-store": "2.0.5",
      "react-native-shake": "3.4.0",
      "react-native-splash-screen": "3.2.0",
      "react-native-sqlite-storage": "4.1.0",
      "react-native-svg": "11.0.1",
      "react-native-tab-view": "2.10.0",
      "react-native-unimodules": "~0.9.0",
      "react-native-uuid": "1.4.9",
      "react-native-web": "0.12.1",
      "react-redux": "7.1.3",
      "readable-stream": "1.0.33",
      "redux": "4.0.4",
      "redux-ignore": "^1.2.5",
      "redux-saga": "1.1.3",
      "redux-thunk": "2.3.0",
      "restful-react": "9.11.0",
      "rn-nodeify": "10.2.0",
      "stream-browserify": "1.0.0",
      "ts-node": "8.6.2",
      "typeorm": "0.2.22",
      "typescript": "3.6.4",
      "vm-browserify": "0.0.4",
      "zoomable-svg": "5.0.1"
  },
  "devDependencies": {
      "@babel/core": "7.6.4",
      "@babel/plugin-proposal-class-properties": "7.8.3",
      "@babel/plugin-proposal-decorators": "7.8.3",
      "@babel/runtime": "7.6.3",
      "@commitlint/cli": "8.3.5",
      "@commitlint/config-conventional": "8.3.4",
      "@react-native-community/eslint-config": "0.0.5",
      "@types/core-js": "2.5.3",
      "@types/enzyme": "3.10.0",
      "@types/enzyme-adapter-react-16": "1.0.6",
      "@types/jest": "24.9.0",
      "@types/react": "16.9.17",
      "@types/react-native": "0.62.2",
      "@types/react-native-i18n": "2.0.0",
      "@types/react-redux": "7.1.7",
      "@types/react-test-renderer": "16.9.1",
      "@typescript-eslint/eslint-plugin": "2.5.0",
      "@typescript-eslint/parser": "2.5.0",
      "babel-eslint": "10.0.3",
      "babel-jest": "24.9.0",
      "core-js": "^3.6.4",
      "enzyme": "3.10.0",
      "enzyme-adapter-react-16": "1.15.1",
      "enzyme-to-json": "3.4.4",
      "eslint": "6.5.1",
      "eslint-formatter-markdown": "1.0.4",
      "eslint-plugin-jsx-a11y": "^6.2.3",
      "eslint-plugin-react": "7.16.0",
      "eslint-plugin-sonarjs": "0.4.0",
      "flow": "0.2.3",
      "husky": "3.0.9",
      "jest": "24.9.0",
      "jest-cucumber": "2.0.11",
      "jest-enzyme": "7.0.2",
      "jest-expo": "^37.0.0",
      "jest-fetch-mock": "3.0.1",
      "jest-html-reporter": "^3.1.0",
      "jest-junit": "9.0.0",
      "jsdom": "^16.2.1",
      "jsdom-global": "3.0.2",
      "lint-staged": "9.4.2",
      "metro": "^0.58.0",
      "metro-react-native-babel-preset": "0.56.0",
      "prettier": "1.18.2",
      "react-addons-test-utils": "15.6.2",
      "react-dom": "16.11.0",
      "react-native-testing-library": "1.11.1",
      "react-native-typescript-transformer": "1.2.13",
      "react-test-renderer": "16.9.0",
      "redux-mock-store": "1.5.3",
      "run-script-os": "1.0.7",
      "sonarqube-scanner": "^2.6.0",
      "ts-jest": "24.3.0"
  },
  "husky": {
      "hooks": {
          "pre-commit": "lint-staged && npm run lint",
          "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
      }
  },
  "lint-staged": {
      "src/**/*.{js,jsx,ts,tsx,}": [
          "npm run pretty",
          "git add"
      ]
  },
  "react-native": {
      "crypto": "react-native-crypto",
      "_stream_transform": "readable-stream/transform",
      "_stream_readable": "readable-stream/readable",
      "_stream_writable": "readable-stream/writable",
      "_stream_duplex": "readable-stream/duplex",
      "_stream_passthrough": "readable-stream/passthrough",
      "stream": "stream-browserify",
      "vm": "vm-browserify"
  },
  "browser": {
      "crypto": "react-native-crypto",
      "_stream_transform": "readable-stream/transform",
